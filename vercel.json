{
  "functions": {
    "src/app/api/projects/*/idea/expand/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/projects/*/evaluation/evaluate/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/projects/*/documents/*/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/projects/*/documents/*/revise/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}
